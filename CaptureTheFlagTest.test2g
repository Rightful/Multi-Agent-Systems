use MAS_Group_28.
use UTbot as knowledge.
use CaptureTheFlag as module.
timeout = 30.

test CaptureTheFlag with
	pre{ true }
	in{
	
	bel(base('red', ID) ,flagState('red', 'home')) leadsto navigate(ID).	
	bel (not(flag(_,_,_)) , lastKnownLocFlag(Pos))leadsto navigate(Pos).
	bel(flag('red',[], Position), not(flagState('red', 'held'))) leadsto navigate(Position). 
	bel(base('blue', Home), flag('red',ID,_), self(ID,_,_))leadsto navigate(Home).		
		
	}
	post{ true }
	
attackbot {
	do CaptureTheFlag. 
}